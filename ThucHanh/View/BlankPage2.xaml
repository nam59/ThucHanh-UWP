<Page
    x:Class="ThucHanh.View.BlankPage2"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:ThucHanh.View"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid>
        <StackPanel HorizontalAlignment="Center">
            <RelativePanel>
                <TextBox Header="Current Files" x:Name="Cur" VerticalAlignment="Top" HorizontalAlignment="Center" Width="250"/>
            </RelativePanel>
            <RelativePanel>
                <TextBox Header="FileContent" x:Name="FileContent" VerticalAlignment="Top" HorizontalAlignment="Center" Width="250" Height="120"/>
            </RelativePanel>
            </StackPanel>
    </Grid>
</Page>
